.agg-result-table {
  .agg-result-detail-cell {
    display: none;
  }

  thead {
    display: none;
  }
}

@media screen and (min-width: @screen-tablet) {
  .agg-result-table {
    .agg-result-detail-cell {
      display: table-cell;
    }

    thead {
      display: table-header-group;
    }
  }
}
